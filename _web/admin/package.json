    "swiper": "^11.2.8",
    "tdesign-icons-vue-next": "^0.3.4",
    "tdesign-vue-next": "^1.10.2",
    "tvision-color": "^1.6.0",
    "vue": "~3.5.13",
    "vue-baidu-map-3x": "^1.0.40",
    "vue-cropper": "^1.1.4",
    "vue-demi": "^0.14.10",
